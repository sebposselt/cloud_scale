extends layout

block content
  h1= title
  p Welcome to #{title}

  mixin cam(id, desc)
    input(type="checkbox", name='camID', value="#{id}")
    |#{desc} <br>

  script(type='text/javascript')  
    var cam_list = #{cams};
  
  
  #form
    form(align="center" action='/', method='get', target="_self")
      each val, index in cam_list
        +cam(index,val)
      input(type='submit', value='Find Cams')
